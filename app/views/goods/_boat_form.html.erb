<%= simple_form_for [@good, @partner] do |f|  %>
  <div class="field">
    <%= f.input :numberber_of_passengers, label: "Quantos passageiros você pretende levar em média?" %>
  </div>
  <div class="field">
    <%= f.input :km_month, label: "Quantas milhas você pretende navegar em média por mês?" %>
  </div>
  <div class="field">
    <%= f.input :frenquency_month, label: "Quantas vezes por mês você pretende usar?" %>
  </div>
  <div class="form-btns">
    <%= f.button :submit, class:'btn btn-partnr-yellow' %>
    <%= link_to "Cancel", good_path(@good), class:'btn btn-outline-partnr-dark-gray', data: {dismiss: 'modal'} %>
  </div>
<% end %>
