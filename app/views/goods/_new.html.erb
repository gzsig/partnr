<div class="modal fade" id="formModal" tabindex="-1" role="dialog" aria-labelledby="formModal" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <% if @good.good_type.capitalize == 'Carro' %>
          <%= render 'car_form' %>
        <% elsif @good.good_type.capitalize == 'Barco' %>
          <%= render 'boat_form' %>
        <% elsif @good.good_type.capitalize == 'Helicoptero' %>
          <%= render 'helicopter_form' %>
        <% elsif @good.good_type.capitalize == 'Jatinho' %>
          <%= render 'jet_form' %>
        <% end %>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-partnr-yellow" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>
