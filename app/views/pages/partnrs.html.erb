<div class="container">
  <% @goods.each do |good| %>
  <div class="my-card d-flex align-content-end flex-wrap">
    <h3><%= good.brand %> <%= good.model %></h3>
    <div >
    <% good.users.each do |user| %>
    <%= cl_image_tag user.profile_picture, height: 100, width: 100, crop: :thumb, gravity: :face, class: 'rounded-circle avatar' %>
    <% end %>
  </div>

  </div>
  <% end %>
</div>
