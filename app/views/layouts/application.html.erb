<!DOCTYPE html>
<html>
  <head>
    <title>Partnr</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <link rel="icon" type="image/x-icon"  href="assets/favicon.ico">
  </head>

  <body>
    <%= render 'shared/navbar' %>
    <% if notice %>
      <p class="notice alert alert-success text-center"><%= notice %></p>
    <% elsif alert %>
      <p class="alert alert alert-danger text-center"><%= alert %></p>
    <% end %>
    <%= javascript_include_tag 'application' %>
    <%= javascript_pack_tag 'application' %>
    <%= yield %>
  </body>
</html>
